{
  "manifest_version": 2,

  "name": "EduBuddy",
  "description": "Quick launch Traversy Media",
  "version": "1.0",
  "icons": { "128": "icon_128.png" },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  //"permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ],
  "background": {
    "js": ["popup.js"]
  }
}
